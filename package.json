  },
  "keywords": [
    "yandex",
    "disk",
    "api"
  ],
  "lint-staged": {
    "+(examples|lib|tests|__mocks__)/*.js": "npm run lint:fix"
  },
  "lint-staged": {
    "+(examples|lib|tests)/*.js": [
      "npm run lint:fix",
      "git add"
    ]
